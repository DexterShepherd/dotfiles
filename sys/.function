workon() {
  . $HOME/.virtualenvs/$1/bin/activate
}

function info(){ #see link in color codes
  GIT=''
  BRANCH=`git branch 2> /dev/null | grep \* | awk '{print $2}'`
  printf "$txtgrn $(hostname)"
  if [[ "$BRANCH" != "" ]]; then
    printf ": $txtcyn$BRANCH"
  fi
  printf "$txtblu in "
}

