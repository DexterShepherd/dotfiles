[user]
	name = Dexter Shepherd
	email = dexterjshepherd@gmail.com
[credential]
	helper = osxkeychain
[push]
	default = simple
[core]
	editor = vim
	excludesfile = /Users/dextershepherd/.gitignore_global
[diff]
	tool = opendiff
[merge]
	tool = opendiff
[pull]
	rebase = true
[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
